{
    "manifest_version": 3,
    "name": "Device Form Filler Extension",
    "version": "1.0",
    "description": "An extension that fills forms for different devices.",
    "permissions": [
        "activeTab",
        "scripting",
        "tabs",
        "webNavigation"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "index.html"
    },
    "host_permissions": [
        "*://*/*"
    ]
}